{
    "family": "flagsmith-task-definition",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::302456015006:role/task-exec-role-5512471",
    "taskRoleArn": "arn:aws:iam::302456015006:role/task-exec-role-5512471",
    "containerDefinitions": [
        {
            "name": "flagsmith-api",
            "image": "302456015006.dkr.ecr.eu-west-2.amazonaws.com/flagsmith-ecr-580125f:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "AWS_REGION",
                    "value": "eu-west-2"
                },
                {
                    "name": "DJANGO_SETTINGS_MODULE",
                    "value": "app.settings.production"
                },
                {
                    "name": "DJANGO_ALLOWED_HOSTS",
                    "value": "*"
                },
                {
                    "name": "ALLOWED_HOSTS",
                    "value": "*"
                }
            ],
            "secrets": [
                {
                    "name": "DATABASE_URL",
                    "valueFrom": "arn:aws:secretsmanager:eu-west-2:302456015006:secret:DATABASE_URL-EMaUaY"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "flagsmith-fargate-eu-west-2-8e1e329",
                    "awslogs-region": "eu-west-2",
                    "awslogs-stream-prefix": "awslogs-flagsmith"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}